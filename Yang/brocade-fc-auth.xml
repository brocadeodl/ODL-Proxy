<?xml version="1.0" encoding="UTF-8"?>
<module name="brocade-fc-auth"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:brcd-fc-auth="urn:brocade.com:mgmt:brocade-fc-auth"
        xmlns:common-def="urn:brocade.com:mgmt:brocade-common-def"
        xmlns:rbridge="urn:brocade.com:mgmt:brocade-rbridge">
  <namespace uri="urn:brocade.com:mgmt:brocade-fc-auth"/>
  <prefix value="brcd-fc-auth"/>
  <import module="brocade-common-def">
    <prefix value="common-def"/>
  </import>
  <import module="brocade-rbridge">
    <prefix value="rbridge"/>
  </import>
  <organization>
    <text>Brocade Communications Systems, Inc.</text>
  </organization>
  <contact>
    <text>Brocade Communications Systems, Inc.
Postal: 130 Holger Way
       San Jose, CA 95134
        U.S.A
Tel: +1-408-333-8000
E-mail: support@Brocade.com
web: www.brocade.com.</text>
  </contact>
  <description>
    <text>Data Model for AAA CLIs</text>
  </description>
  <revision date="2010-10-21">
    <description>
      <text>Removed the max-elements attribute for list</text>
    </description>
  </revision>
  <typedef name="authtype">
    <type name="enumeration">
      <enum name="dh-chap"/>
    </type>
    <description>
      <text>Authentication types</text>
    </description>
  </typedef>
  <typedef name="fcsp-authtype">
    <type name="authtype"/>
    <description>
      <text>FCSP authentication types</text>
    </description>
  </typedef>
  <typedef name="hashtype">
    <type name="enumeration">
      <enum name="sha1"/>
      <enum name="md5"/>
      <enum name="all"/>
    </type>
    <description>
      <text>Hash types</text>
    </description>
  </typedef>
  <typedef name="fcsp-hashtype">
    <type name="hashtype"/>
    <description>
      <text>FCSP supported hash types</text>
    </description>
  </typedef>
  <typedef name="switch-policy-state">
    <type name="enumeration">
      <enum name="on"/>
      <enum name="off"/>
      <enum name="active"/>
      <enum name="passive"/>
    </type>
    <description>
      <text>Switch authentication policy mode</text>
    </description>
  </typedef>
  <typedef name="fcsp-switch-policy-state">
    <type name="switch-policy-state"/>
  </typedef>
  <typedef name="secpolicy-option">
    <type name="string">
      <pattern value="SCC_POLICY"/>
    </type>
  </typedef>
  <typedef name="secpolicy-type">
    <type name="secpolicy-option"/>
  </typedef>
  <typedef name="secpolicy-action-type">
    <type name="enumeration">
      <enum name="activate">
        <value value="0"/>
      </enum>
    </type>
  </typedef>
  <typedef name="fc-node-name-type">
    <type name="union">
      <type name="common-def:wwn-type"/>
      <type name="string"/>
      <type name="common-def:rbridge-id-type"/>
    </type>
  </typedef>
  <grouping name="fcsp-group">
    <container name="fcsp">
      <container name="auth">
        <container name="proto">
          <leaf name="auth-type">
            <type name="fcsp-authtype"/>
            <default value="dh-chap"/>
          </leaf>
          <leaf name="group">
            <type name="string">
              <pattern value="[0-4\*]">
                <error-message>
                  <value>Group value should be
 in the range 0-4 or *</value>
                </error-message>
              </pattern>
            </type>
            <default value="*"/>
          </leaf>
          <leaf name="hash">
            <type name="fcsp-hashtype"/>
            <default value="all"/>
          </leaf>
        </container>
        <container name="policy">
          <leaf name="switch">
            <type name="fcsp-switch-policy-state"/>
            <default value="passive"/>
          </leaf>
        </container>
      </container>
    </container>
  </grouping>
  <container name="fcsp">
    <container name="auth-secret"/>
  </container>
  <augment target-node="/rbridge:rbridge-id">
    <uses name="fcsp-group"/>
  </augment>
  <container name="fcsp-sa">
    <uses name="fcsp-group"/>
  </container>
  <augment target-node="/common-def:no">
    <container name="fcsp">
      <container name="auth-secret"/>
    </container>
  </augment>
  <augment target-node="/common-def:show">
    <container name="fcsp">
      <description>
        <text>Displaying FCSP auth secrets</text>
      </description>
      <container name="authsecret"/>
    </container>
  </augment>
  <grouping name="secpolicy-group">
    <container name="secpolicy">
      <container name="defined-policy">
        <description>
          <text>Set the defined policy</text>
        </description>
        <list name="policies">
          <key value="policy"/>
          <leaf name="policy">
            <type name="secpolicy-type"/>
          </leaf>
          <list name="member-entry">
            <key value="member"/>
            <leaf name="member">
              <type name="string"/>
              <description>
                <text>Indicates the switch WWN</text>
              </description>
            </leaf>
          </list>
        </list>
      </container>
      <container name="active-policy">
        <description>
          <text>Set the Active policy</text>
        </description>
        <list name="policies">
          <key value="policy"/>
          <leaf name="policy">
            <type name="secpolicy-type"/>
          </leaf>
          <list name="member-entry">
            <key value="member"/>
            <leaf name="member">
              <type name="string"/>
              <description>
                <text>Indicates the switch WWN</text>
              </description>
            </leaf>
          </list>
        </list>
      </container>
    </container>
  </grouping>
  <container name="secpolicy">
    <container name="action"/>
  </container>
  <augment target-node="/rbridge:rbridge-id">
    <uses name="secpolicy-group"/>
  </augment>
  <container name="secpolicy-sa">
    <uses name="secpolicy-group"/>
  </container>
</module>
