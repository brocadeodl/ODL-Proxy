<?xml version="1.0" encoding="UTF-8"?>
<module name="brocade-chassis"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:chassis="urn:brocade.com:mgmt:brocade-chassis"
        xmlns:rbridge="urn:brocade.com:mgmt:brocade-rbridge"
        xmlns:common-def="urn:brocade.com:mgmt:brocade-common-def"
        xmlns:vcs="urn:brocade.com:mgmt:brocade-vcs">
  <namespace uri="urn:brocade.com:mgmt:brocade-chassis"/>
  <prefix value="chassis"/>
  <import module="brocade-rbridge">
    <prefix value="rbridge"/>
  </import>
  <import module="brocade-common-def">
    <prefix value="common-def"/>
  </import>
  <import module="brocade-vcs">
    <prefix value="vcs"/>
  </import>
  <organization>
    <text>Brocade Communications Systems, Inc.</text>
  </organization>
  <contact>
    <text>Brocade Communications Systems, Inc.
Postal: 130 Holger Way
        San Jose, CA 95134
         U.S.A
Tel: +1-408-333-8000
E-mail: support@Brocade.com
web: www.brocade.com.</text>
  </contact>
  <description>
    <text>Module for implementing switch configurable
enable/disable CLIs</text>
  </description>
  <revision date="2011-04-11">
    <description>
      <text>Initial version of this management module.</text>
    </description>
  </revision>
  <grouping name="chassis-config-cmds">
    <container name="chassis">
      <leaf name="virtual-ip">
        <type name="common-def:ipv4-address-prefix-type"/>
      </leaf>
      <leaf name="virtual-ipv6">
        <type name="common-def:ipv6-address-prefix"/>
      </leaf>
      <container name="oper-address">
        <leaf name="virtual-oper-Vip-address">
          <type name="common-def:ipv4-address-prefix-type"/>
          <config value="false"/>
          <description>
            <text>The assigned chassis IPv4 address.</text>
          </description>
        </leaf>
        <leaf-list name="virtual-oper-VipV6-address">
          <type name="string"/>
          <config value="false"/>
          <description>
            <text>The assigned chassis IPv6 address.</text>
          </description>
        </leaf-list>
      </container>
    </container>
  </grouping>
  <augment target-node="/rbridge:rbridge-id">
    <uses name="chassis-config-cmds"/>
  </augment>
  <container name="hide-virtual-ip-holder">
    <when condition='/vcs:vcsmode/vcs:vcs-mode = "false"'/>
    <uses name="chassis-config-cmds"/>
  </container>
  <container name="chassis">
    <container name="beacon"/>
    <container name="fan">
      <container name="airflow-direction"/>
    </container>
  </container>
</module>
