  <grouping name="common-maxas-limit-group">
    <container name="maxas-limit">
      <container name="in">
        <choice name="ch-maxas-limit">
          <case name="ca-maxas-limit-enable">
            <leaf name="num-as-in-as-path">
              <type name="num-as-in-as-path"/>
            </leaf>
          </case>
          <case name="ca-maxas-limit-disable">
            <leaf name="maxas-limit-disable">
              <type name="empty"/>
            </leaf>
          </case>
        </choice>
      </container>
    </container>
  </grouping>
  <grouping name="af-static-network-cmds">
    <list name="static-network">
      <key value="static-network-address"/>
      <leaf name="static-network-address">
        <type name="inet:ipv4-prefix"/>
      </leaf>
      <leaf name="static-network-distance">
        <type name="sdistance"/>
      </leaf>
    </list>
  </grouping>
  <grouping name="common-maximum-paths-grouping">
    <container name="maximum-paths">
      <leaf name="load-sharing-value">
        <type name="uint32">
          <range value="1..8"/>
        </type>
      </leaf>
      <leaf name="ebgp">
        <type name="ebgp-paths"/>
      </leaf>
      <leaf name="ibgp">
        <type name="ibgp-paths"/>
      </leaf>
      <leaf name="use-load-sharing">
        <type name="empty"/>
      </leaf>
    </container>
  </grouping>
  <grouping name="af-ipv4-network-cmds">
    <list name="network">
      <key value="network-ipv4-address"/>
      <leaf name="network-ipv4-address">
        <type name="inet:ipv4-prefix"/>
      </leaf>
      <uses name="af-common-network-cmds"/>
    </list>
  </grouping>
  <grouping name="af-common-network-cmds">
    <leaf name="network-weight">
      <type name="absolute-decimal-number"/>
    </leaf>
    <leaf name="backdoor">
      <type name="empty"/>
    </leaf>
    <leaf name="network-route-map">
      <type name="rmap-type"/>
    </leaf>
  </grouping>
  <grouping name="address-family-group">
    <container name="address-family">
      <container name="ipv4">
        <container name="ipv4-unicast">
          <presence value="true"/>
          <uses name="af-ipv4-uc-cmds"/>
        </container>
      </container>
    </container>
  </grouping>
  <grouping name="af-ipv4-neighbor-cmds">
    <container name="af-ipv4-neighbor-peergroup-holder">
      <list name="af-ipv4-neighbor-peergroup">
        <key value="af-ipv4-neighbor-peergroup-name"/>
        <leaf name="af-ipv4-neighbor-peergroup-name">
          <type name="bgp-peergroup"/>
        </leaf>
        <uses name="af-neighbor-cmds"/>
      </list>
    </container>
    <container name="af-ipv4-neighbor-address-holder">
      <list name="af-ipv4-neighbor-address">
        <key value="af-ipv4-neighbor-address"/>
        <leaf name="af-ipv4-neighbor-address">
          <type name="inet:ip-address"/>
        </leaf>
        <uses name="af-neighbor-cmds"/>
      </list>
    </container>
  </grouping>
  <grouping name="af-neighbor-cmds">
    <leaf name="activate">
      <type name="empty"/>
    </leaf>
    <container name="filter-list">
      <container name="direction-in">
        <leaf name="filter-list-direction-in-acl-name">
          <type name="ip-filter-name"/>
        </leaf>
        <leaf name="filter-list-name-direction-in">
          <type name="empty"/>
        </leaf>
      </container>
      <container name="direction-out">
        <leaf name="filter-list-direction-out-acl-name">
          <type name="ip-filter-name"/>
        </leaf>
        <leaf name="filter-list-name-direction-out">
          <type name="empty"/>
        </leaf>
      </container>
    </container>
    <container name="maximum-prefix">
      <leaf name="max-prefix-limit">
        <type name="max-prefix-limit"/>
      </leaf>
      <container name="threshold-holder">
        <leaf name="threshold">
          <type name="max-prefix-threshold"/>
        </leaf>
        <leaf name="teardown">
          <type name="empty"/>
        </leaf>
      </container>
    </container>
    <container name="default-originate">
      <leaf name="default-originate-status">
        <type name="empty"/>
      </leaf>
      <leaf name="default-originate-route-map">
        <type name="rmap-type"/>
      </leaf>
    </container>
    <container name="prefix-list">
      <description>
        <text>either prefix list or distribution-list</text>
      </description>
      <container name="direction-in">
        <leaf name="prefix-list-direction-in-prefix-name">
          <type name="nei-prefix-list-filter"/>
        </leaf>
        <leaf name="prefix-list-direction-in">
          <type name="empty"/>
        </leaf>
      </container>
      <container name="direction-out">
        <leaf name="prefix-list-direction-out-prefix-name">
          <type name="nei-prefix-list-filter"/>
        </leaf>
        <leaf name="prefix-list-direction-out">
          <type name="empty"/>
        </leaf>
      </container>
    </container>
    <container name="neighbor-route-map">
      <container name="neighbor-route-map-direction-in">
        <leaf name="neighbor-route-map-name-direction-in">
          <type name="common-def:name-string64"/>
        </leaf>
      </container>
      <container name="neighbor-route-map-direction-out">
        <leaf name="neighbor-route-map-name-direction-out">
          <type name="common-def:name-string64"/>
        </leaf>
      </container>
    </container>
    <leaf name="route-reflector-client">
      <type name="empty"/>
    </leaf>
    <container name="send-community">
      <description>
        <text>TODO default is standard.if extended is set, then the show run has to render the value as both</text>
      </description>
      <leaf name="send-community-status">
        <type name="empty"/>
      </leaf>
      <leaf name="both">
        <type name="empty"/>
      </leaf>
      <leaf name="extended">
        <type name="empty"/>
      </leaf>
      <leaf name="standard">
        <type name="empty"/>
      </leaf>
    </container>
    <container name="unsuppress-map">
      <leaf name="map-name">
        <type name="rmap-type"/>
      </leaf>
    </container>
    <leaf name="af-nei-weight">
      <type name="nei-weight"/>
    </leaf>
  </grouping>
  <grouping name="router-bgp-neighbor-attributes">
    <leaf name="description">
      <type name="neighbor-description"/>
    </leaf>
    <container name="shutdown">
      <leaf name="shutdown-status">
        <type name="empty"/>
      </leaf>
      <leaf name="generate-rib-out">
        <type name="empty"/>
      </leaf>
    </container>
    <container name="local-as">
      <leaf name="local-as-value">
        <type name="local-as-num"/>
      </leaf>
      <leaf name="no-prepend">
        <type name="empty"/>
      </leaf>
    </container>
    <container name="next-hop-self">
      <choice name="ch-next-hop-self-type">
        <case name="ca-next-hop-self-always">
          <leaf name="next-hop-self-always">
            <type name="empty"/>
          </leaf>
        </case>
        <case name="ca-next-hop-self-status">
          <leaf name="next-hop-self-status">
            <type name="empty"/>
          </leaf>
        </case>
      </choice>
    </container>
    <container name="advertisement-interval">
      <leaf name="value">
        <type name="adv-value"/>
      </leaf>
    </container>
    <container name="ebgp-multihop">
      <choice name="ch-ebgp-multihop-type">
        <case name="ca-ebgp-multihop-flag">
          <leaf name="ebgp-multihop-flag">
            <type name="empty"/>
          </leaf>
        </case>
        <case name="ca-ebgp-multihop-count">
          <leaf name="ebgp-multihop-count">
            <type name="uint32">
              <range value="1..255"/>
            </type>
          </leaf>
        </case>
      </choice>
    </container>
    <uses name="common-maxas-limit-group"/>
    <leaf name="password">
      <type name="bgp-password"/>
    </leaf>
    <container name="enforce-first-as">
      <leaf name="nei-enforce-first-as">
        <type name="empty"/>
      </leaf>
      <leaf name="nei-enforce-first-as-disable">
        <type name="empty"/>
      </leaf>
    </container>
    <container name="neighbor-capability">
      <container name="as4">
        <leaf name="neighbor-as4-enable">
          <type name="empty"/>
        </leaf>
        <leaf name="neighbor-as4-disable">
          <type name="empty"/>
        </leaf>
      </container>
    </container>
    <container name="timers">
      <leaf name="nei-keep-alive">
        <type name="nei-keep-alive"/>
      </leaf>
      <leaf name="nei-hold-time">
        <type name="nei-hold-time"/>
      </leaf>
    </container>
    <container name="update-source">
      <choice name="ch-update-source">
        <case name="ca-ipv4">
          <leaf name="sip-ipv4-address">
            <type name="sip-ipv4-address"/>
          </leaf>
        </case>
        <case name="ca-eth">
          <container name="ethernet-interface">
            <leaf name="interface-type">
              <type name="enumeration">
                <enum name="gigabitethernet">
                  <value value="1"/>
                </enum>
                <enum name="tengigabitethernet">
                  <value value="2"/>
                </enum>
                <enum name="fortygigabitethernet">
                  <value value="3"/>
                </enum>
              </type>
            </leaf>
            <leaf name="ethernet">
              <type name="update-source-ethernet-type"/>
            </leaf>
          </container>
        </case>
        <case name="ca-loopback">
          <leaf name="loopback">
            <type name="loopback-interface"/>
          </leaf>
        </case>
        <case name="ca-ve">
          <leaf name="ve-interface">
            <type name="ve-interface"/>
          </leaf>
        </case>
      </choice>
    </container>
    <leaf name="remove-private-as">
      <type name="empty"/>
    </leaf>
    <leaf name="as-override">
      <type name="empty"/>
    </leaf>
    <container name="soft-reconfiguration">
      <leaf name="inbound">
        <type name="empty"/>
      </leaf>
    </container>
  </grouping>
  <grouping name="router-bgp-neighbor-cmds">
    <container name="peer-grps">
      <list name="neighbor-peer-grp">
        <key value="router-bgp-neighbor-peer-grp"/>
        <leaf name="router-bgp-neighbor-peer-grp">
          <type name="bgp-peergroup"/>
        </leaf>
        <leaf name="peer-group-name">
          <type name="empty"/>
        </leaf>
        <leaf name="remote-as">
          <type name="bgp-remote-as"/>
        </leaf>
        <uses name="router-bgp-neighbor-attributes"/>
      </list>
    </container>
    <container name="neighbor-ips">
      <list name="neighbor-addr">
        <key value="router-bgp-neighbor-address"/>
        <leaf name="router-bgp-neighbor-address">
          <type name="inet:ip-address"/>
        </leaf>
        <leaf name="remote-as">
          <type name="bgp-remote-as"/>
        </leaf>
        <leaf name="associate-peer-group">
          <type name="bgp-peergroup"/>
        </leaf>
        <uses name="router-bgp-neighbor-attributes"/>
      </list>
    </container>
  </grouping>
  <grouping name="af-redistrubute-unicast-grouping">
    <container name="redistribute">
      <container name="connected">
        <leaf name="redistribute-connected">
          <type name="empty"/>
        </leaf>
        <leaf name="unicast-metric">
          <type name="conn-metric"/>
        </leaf>
        <leaf name="redistribute-route-map">
          <type name="rmap-type"/>
        </leaf>
      </container>
      <container name="ospf">
        <leaf name="redistribute-ospf">
          <type name="empty"/>
        </leaf>
        <container name="match">
          <leaf name="ospf-internal">
            <type name="empty"/>
          </leaf>
          <leaf name="ospf-external1">
            <type name="empty"/>
          </leaf>
          <leaf name="ospf-external2">
            <type name="empty"/>
          </leaf>
        </container>
        <leaf name="ospf-metric">
          <type name="conn-metric"/>
        </leaf>
        <leaf name="ospf-route-map">
          <type name="rmap-type"/>
        </leaf>
      </container>
      <container name="static">
        <leaf name="redistribute-static">
          <type name="empty"/>
        </leaf>
        <leaf name="unicast-static-metric">
          <type name="conn-metric"/>
        </leaf>
        <leaf name="static-route-map">
          <type name="rmap-type"/>
        </leaf>
      </container>
    </container>
  </grouping>
  <grouping name="af-aggregate-ipv4-address-cmd">
    <list name="aggregate-address">
      <key value="aggregate-ip-prefix"/>
      <leaf name="aggregate-ip-prefix">
        <type name="inet:ipv4-prefix"/>
      </leaf>
      <uses name="common-aggregate-address-grouping"/>
    </list>
  </grouping>
  <grouping name="common-aggregate-address-grouping">
    <leaf name="advertise-map">
      <type name="advertise-map"/>
    </leaf>
    <leaf name="as-set">
      <type name="empty"/>
    </leaf>
    <leaf name="attribute-map">
      <type name="attribute-map"/>
    </leaf>
    <leaf name="summary-only">
      <type name="empty"/>
    </leaf>
    <leaf name="suppress-map">
      <type name="suppress-map"/>
    </leaf>
  </grouping>
  <grouping name="common-multipath-grouping">
    <container name="multipath">
      <leaf name="multipath_ebgp">
        <type name="empty"/>
      </leaf>
      <leaf name="multipath_ibgp">
        <type name="empty"/>
      </leaf>
      <leaf name="multi-as">
        <type name="empty"/>
      </leaf>
    </container>
  </grouping>
  <grouping name="af-ipv4-uc-and-vrf-cmds">
    <container name="af-ipv4-uc-and-vrf-cmds-call-point-holder">
      <leaf name="bgp-redistribute-internal">
        <type name="empty"/>
      </leaf>
      <uses name="af-redistrubute-unicast-grouping"/>
    </container>
    <uses name="af-static-network-cmds"/>
  </grouping>
  <grouping name="af-ipv4-uc-and-mc-cmds">
    <uses name="af-ipv4-neighbor-cmds"/>
    <container name="af-common-cmds-holder">
      <uses name="af-common-cmds"/>
    </container>
  </grouping>
  <grouping name="af-ipv4-uc-cmds">
    <uses name="af-ipv4-uc-and-vrf-cmds">
      <refine target-node="af-ipv4-uc-and-vrf-cmds-call-point-holder"/>
    </uses>
    <uses name="af-aggregate-ipv4-address-cmd">
      <refine target-node="aggregate-address"/>
    </uses>
    <uses name="af-ipv4-network-cmds">
      <refine target-node="network"/>
    </uses>
    <uses name="af-ipv4-uc-and-mc-cmds">
      <refine target-node="af-common-cmds-holder"/>
    </uses>
    <leaf name="next-hop-recursion">
      <type name="empty"/>
    </leaf>
  </grouping>
  <grouping name="af-basic-cmds">
    <uses name="common-maximum-paths-grouping"/>
    <uses name="common-multipath-grouping"/>
    <leaf name="always-propagate">
      <type name="empty"/>
    </leaf>
    <leaf name="default-information-originate">
      <type name="empty"/>
    </leaf>
    <leaf name="rib-route-limit">
      <type name="decimal-number"/>
    </leaf>
  </grouping>
  <grouping name="af-common-cmds">
    <uses name="af-basic-cmds"/>
    <leaf name="client-to-client-reflection">
      <type name="empty"/>
    </leaf>
    <container name="dampening">
      <choice name="ch-dampening-source">
        <case name="ca-dampening-flag">
          <leaf name="dampening-flag">
            <type name="empty"/>
            <description>
              <text>This Flag is to indicate dampening feature  is enabled</text>
            </description>
          </leaf>
        </case>
        <case name="ca-dampening-specify-values">
          <container name="values">
            <leaf name="half-time">
              <type name="htime"/>
            </leaf>
            <leaf name="reuse-value">
              <type name="damp-reuse-value"/>
            </leaf>
            <leaf name="start-suppress-time">
              <type name="damp-start-suppress-value"/>
            </leaf>
            <leaf name="max-suppress-time">
              <type name="damp-max-suppress-value"/>
            </leaf>
          </container>
        </case>
        <case name="ca-dampening-route-map">
          <leaf name="dampening-route-map">
            <type name="rmap-type"/>
          </leaf>
        </case>
      </choice>
    </container>
    <leaf name="default-metric">
      <type name="decimal-number"/>
    </leaf>
    <leaf name="next-hop-enable-default">
      <type name="empty"/>
    </leaf>
    <container name="table-map">
      <leaf name="table-map-route-map">
        <type name="rmap-type"/>
      </leaf>
    </container>
    <leaf name="update-time">
      <type name="update-time-type"/>
    </leaf>
  </grouping>
  <grouping name="bgp-cmds">
    <list name="bgp">
      <key value="vrf-name"/>
      <leaf name="vrf-name">
        <type name="string">
          <length value="1..255"/>
        </type>
      </leaf>
      <container name="router-bgp-cmds-holder">
        <uses name="router-bgp-cmds"/>
        <uses name="address-family-group"/>
      </container>
    </list>
  </grouping>
  <grouping name="router-bgp-cmds">
    <container name="router-bgp-attributes">
      <leaf name="local-as">
        <type name="local-as-type"/>
      </leaf>
      <leaf name="always-compare-med">
        <type name="empty"/>
      </leaf>
      <leaf name="compare-med-empty-aspath">
        <type name="empty"/>
      </leaf>
      <leaf name="med-missing-as-worst">
        <type name="empty"/>
      </leaf>
      <leaf name="as-path-ignore">
        <type name="empty"/>
      </leaf>
      <leaf name="compare-routerid">
        <type name="empty"/>
      </leaf>
      <leaf name="install-igp-cost">
        <type name="empty"/>
      </leaf>
      <container name="cluster-id">
        <choice name="ch-cluster-id">
          <case name="ca-cluster-id">
            <leaf name="cluster-id-value">
              <type name="decimal-number"/>
            </leaf>
          </case>
          <case name="ca-cluster-id-ipv4-address">
            <leaf name="cluster-id-ipv4-address">
              <type name="inet:ipv4-address"/>
            </leaf>
          </case>
        </choice>
      </container>
      <leaf name="default-local-preference">
        <type name="decimal-number"/>
      </leaf>
      <container name="distance">
        <leaf name="ext-route-distance">
          <type name="ext-route-distance"/>
        </leaf>
        <leaf name="int-route-distance">
          <type name="int-route-distance"/>
        </leaf>
        <leaf name="lcl-route-distance">
          <type name="lcl-route-distance"/>
        </leaf>
      </container>
      <container name="capability">
        <leaf name="as4-enable">
          <type name="empty"/>
        </leaf>
      </container>
      <container name="maxas-limit">
        <container name="in">
          <leaf name="num-as-in-path">
            <type name="num-as-in-path"/>
          </leaf>
        </container>
      </container>
      <leaf name="enforce-first-as">
        <type name="empty"/>
      </leaf>
      <leaf name="fast-external-fallover">
        <type name="empty"/>
      </leaf>
      <container name="timers">
        <leaf name="keep-alive">
          <type name="nei-keep-alive"/>
        </leaf>
        <leaf name="hold-time">
          <type name="nei-hold-time"/>
        </leaf>
      </container>
      <leaf name="log-dampening-debug">
        <type name="empty"/>
      </leaf>
      <uses name="router-bgp-neighbor-cmds"/>
    </container>
  </grouping>
  <augment target-node="/rbridge:rbridge-id/rbridge:router">
    <uses name="bgp-cmds"/>
  </augment>
</module>
