  <typedef name="threshold-monitor-state-enum">
    <type name="enumeration">
      <enum name="No-Change">
        <value value="0"/>
      </enum>
      <enum name="In-Range">
        <value value="1"/>
      </enum>
      <enum name="Out-Of-Range">
        <value value="2"/>
      </enum>
    </type>
  </typedef>
  <typedef name="threshold-monitor-pause-enum">
    <type name="enumeration">
      <enum name="Paused">
        <value value="0"/>
      </enum>
      <enum name="Monitoring">
        <value value="1"/>
      </enum>
    </type>
  </typedef>
  <typedef name="threshold-monitor-area">
    <type name="enumeration">
      <enum name="MissingTerminationCharacter">
        <value value="0"/>
      </enum>
      <enum name="CRCAlignErrors">
        <value value="1"/>
      </enum>
      <enum name="SymbolErrors">
        <value value="2"/>
      </enum>
      <enum name="IFG">
        <value value="3"/>
      </enum>
      <enum name="CFI">
        <value value="4"/>
      </enum>
    </type>
  </typedef>
  <typedef name="sfp-type">
    <type name="enumeration">
      <enum name="1GSR">
        <value value="0"/>
      </enum>
      <enum name="1GLR">
        <value value="1"/>
      </enum>
      <enum name="10GSR">
        <value value="2"/>
      </enum>
      <enum name="10GLR">
        <value value="3"/>
      </enum>
      <enum name="10GUSR">
        <value value="4"/>
      </enum>
      <enum name="QSFP">
        <value value="5"/>
      </enum>
    </type>
  </typedef>
  <augment target-node="/common-def:show/common-def:defaults">
    <container name="threshold"/>
  </augment>
</module>
