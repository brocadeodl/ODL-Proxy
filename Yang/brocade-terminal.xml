<?xml version="1.0" encoding="UTF-8"?>
<module name="brocade-terminal"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:terminal="urn:brocade.com:mgmt:brocade-terminal">
  <namespace uri="urn:brocade.com:mgmt:brocade-terminal"/>
  <prefix value="terminal"/>
  <organization>
    <text>Brocade Communications Systems, Inc.</text>
  </organization>
  <contact>
    <text>Brocade Communications Systems, Inc.
Postal: 130 Holger Way
        San Jose, CA 95134
         U.S.A
Tel: +1-408-333-8000
E-mail: support@Brocade.com
web: www.brocade.com.</text>
  </contact>
  <description>
    <text>This management submodule is an instrumentation to 
manage CLI terminal related features.</text>
  </description>
  <revision date="2011-04-18">
    <description>
      <text>Initial version of the management module terminal.</text>
    </description>
  </revision>
  <typedef name="terminal-type">
    <type name="enumeration">
      <enum name="vty">
        <value value="2"/>
      </enum>
    </type>
  </typedef>
  <container name="terminal-cfg">
    <list name="line">
      <key value="sessionid"/>
      <max-elements value="1"/>
      <leaf name="sessionid">
        <type name="terminal-type"/>
      </leaf>
      <leaf name="exec-timeout">
        <type name="uint32">
          <range value="0..136">
            <error-message>
              <value>CLI session idle time must be in the 
range 0 - 136 minutes</value>
            </error-message>
          </range>
        </type>
      </leaf>
    </list>
  </container>
</module>
