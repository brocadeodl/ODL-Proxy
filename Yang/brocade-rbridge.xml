<?xml version="1.0" encoding="UTF-8"?>
<module name="brocade-rbridge"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:rbridge="urn:brocade.com:mgmt:brocade-rbridge">
  <namespace uri="urn:brocade.com:mgmt:brocade-rbridge"/>
  <prefix value="rbridge"/>
  <organization>
    <text>Brocade Communications Systems, Inc.</text>
  </organization>
  <contact>
    <text>Brocade Communications Systems, Inc.
Postal: 130 Holger Way
        San Jose, CA 95134
         U.S.A
Tel: +1-408-333-8000
E-mail: support@Brocade.com 
web: www.brocade.com.</text>
  </contact>
  <description>
    <text>This management module is an instrumentation to manage 
rbridge-id feature.</text>
  </description>
  <revision date="2011-06-21">
    <description>
      <text>Initial version of this management module
brocade-rbridge.</text>
    </description>
  </revision>
  <typedef name="raslog-console">
    <type name="enumeration">
      <enum name="INFO">
        <value value="1"/>
      </enum>
      <enum name="ERROR">
        <value value="2"/>
      </enum>
      <enum name="WARNING">
        <value value="3"/>
      </enum>
      <enum name="CRITICAL">
        <value value="4"/>
      </enum>
    </type>
  </typedef>
  <list name="rbridge-id">
    <key value="rbridge-id"/>
    <leaf name="rbridge-id">
      <type name="uint32"/>
    </leaf>
    <leaf name="swbd-number">
      <type name="int32"/>
      <config value="false"/>
    </leaf>
    <container name="interface-nodespecific">
      <leaf name="ns-vlan">
        <type name="int32"/>
      </leaf>
      <leaf name="ns-ethernet">
        <type name="int32"/>
      </leaf>
    </container>
    <container name="ip">
      <description>
        <text>Internet Protoccol (IP). </text>
      </description>
    </container>
    <container name="ipv6">
      <description>
        <text>Internet Protoccol (IPv6). </text>
      </description>
    </container>
    <container name="router">
      <description>
        <text>The routing system.</text>
      </description>
    </container>
    <container name="switch-attributes">
      <leaf name="chassis-name">
        <type name="string">
          <length value="1..30"/>
        </type>
      </leaf>
      <leaf name="host-name">
        <type name="string">
          <length value="1..30"/>
        </type>
      </leaf>
    </container>
  </list>
</module>
