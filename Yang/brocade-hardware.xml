<?xml version="1.0" encoding="UTF-8"?>
<module name="brocade-hardware"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:hardware="urn:brocade.com:mgmt:brocade-hardware"
        xmlns:brocade-vcs="urn:brocade.com:mgmt:brocade-vcs"
        xmlns:rbridge="urn:brocade.com:mgmt:brocade-rbridge"
        xmlns:interface="urn:brocade.com:mgmt:brocade-interface">
  <namespace uri="urn:brocade.com:mgmt:brocade-hardware"/>
  <prefix value="hardware"/>
  <import module="brocade-vcs">
    <prefix value="brocade-vcs"/>
  </import>
  <import module="brocade-rbridge">
    <prefix value="rbridge"/>
  </import>
  <import module="brocade-interface">
    <prefix value="interface"/>
  </import>
  <organization>
    <text>Brocade Communications Systems, Inc.</text>
  </organization>
  <contact>
    <text>Brocade Communications Systems, Inc.
Postal: 130 Holger Way
        San Jose, CA 95134
         U.S.A
Tel: +1-408-333-8000
E-mail: support@Brocade.com
web: www.brocade.com.</text>
  </contact>
  <description>
    <text>This hardware management module is used to manage connectors.
It can be used to breakout the connectors and set the type
of the connectors.</text>
  </description>
  <revision date="2012-12-20">
    <description>
      <text>Initial version of this management module.</text>
    </description>
  </revision>
  <typedef name="u-port-type">
    <type name="enumeration">
      <enum name="ethernet">
        <value value="1"/>
      </enum>
      <enum name="fibre-channel">
        <value value="2"/>
      </enum>
    </type>
  </typedef>
  <container name="hardware">
    <list name="connector">
      <key value="name"/>
      <leaf name="name">
        <type name="interface:interface-type"/>
        <mandatory value="true"/>
      </leaf>
      <container name="sfp">
        <leaf name="breakout">
          <type name="empty"/>
        </leaf>
      </container>
    </list>
  </container>
</module>
