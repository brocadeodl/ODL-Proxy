  <typedef name="ospf-area-id">
    <type name="string">
      <pattern value="((([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\.)(([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\.){2}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5]))|(([0-9])|([1-9]([0-9]{1,8}))|([1]([0-9]{1,9}))|([2][0]([0-9]{1,8}))|([2][1][0-3]([0-9]{1,7}))|([2][1][4][0-6]([0-9]{1,6}))|([2][1][4][7][0-3]([0-9]{1,5}))|([2][1][4][7][4][0-7]([0-9]{1,4}))|([2][1][4][7][4][8][0-2]([0-9]{1,3}))|([2][1][4][7][4][8][3][0-5]([0-9]{1,2}))|([2][1][4][7][4][8][3][6][0-3][0-9])|([2][1][4][7][4][8][3][6][4][0-7]))"/>
    </type>
  </typedef>
  <typedef name="ospf-auth-psswd-string">
    <type name="string"/>
  </typedef>
  <typedef name="big-metric">
    <type name="uint32"/>
  </typedef>
  <typedef name="range-metric">
    <type name="uint32"/>
  </typedef>
  <typedef name="band-width">
    <type name="uint32">
      <range value="1..4294967"/>
    </type>
    <units name="megabits per second"/>
  </typedef>
  <typedef name="metric-type">
    <type name="enumeration">
      <enum name="type1">
        <value value="1"/>
      </enum>
      <enum name="type2">
        <value value="2"/>
      </enum>
    </type>
  </typedef>
  <typedef name="route-option-type">
    <type name="enumeration">
      <enum name="all">
        <value value="1"/>
      </enum>
      <enum name="connected">
        <value value="2"/>
      </enum>
      <enum name="static">
        <value value="4"/>
      </enum>
    </type>
  </typedef>
  <typedef name="route-type-enum">
    <type name="enumeration">
      <enum name="external">
        <value value="1"/>
      </enum>
      <enum name="inter-area">
        <value value="2"/>
      </enum>
      <enum name="intra-area">
        <value value="3"/>
      </enum>
    </type>
  </typedef>
  <typedef name="match-set-metric-option-type">
    <type name="enumeration">
      <enum name="match-metric">
        <value value="1"/>
      </enum>
      <enum name="set-metric">
        <value value="2"/>
      </enum>
    </type>
  </typedef>
  <typedef name="key-type">
    <type name="uint32">
      <range value="0|2|255"/>
    </type>
  </typedef>
  <typedef name="ospf-hexstring">
    <type name="string">
      <pattern value="[0-9a-fA-F]{1,8}"/>
    </type>
  </typedef>
  <typedef name="ospf-ipv4-address">
    <type name="inet:ipv4-address"/>
  </typedef>
  <typedef name="ospf-ipv4-prefix">
    <type name="inet:ipv4-prefix"/>
  </typedef>
  <typedef name="ospf-link-state-id">
    <type name="inet:ipv4-address"/>
  </typedef>
  <typedef name="ospf-adv-router-id">
    <type name="inet:ipv4-address"/>
  </typedef>
  <typedef name="ospf-router-id">
    <type name="inet:ipv4-address"/>
  </typedef>
  <typedef name="ospf-lsdb-index">
    <type name="uint32"/>
  </typedef>
  <typedef name="ospf-show-area-id-num-range">
    <type name="uint32">
      <range value="0..2147483647"/>
    </type>
  </typedef>
  <typedef name="ospf-show-area-id-ip-range">
    <type name="inet:ipv4-address"/>
  </typedef>
  <typedef name="ospf-show-area-id-range">
    <type name="union">
      <type name="ospf-show-area-id-num-range"/>
      <type name="ospf-show-area-id-ip-range"/>
    </type>
  </typedef>
  <typedef name="ospf-neighbor-rid">
    <type name="inet:ipv4-address"/>
  </typedef>
  <typedef name="ospf-entry-index">
    <type name="uint32"/>
  </typedef>
  <typedef name="ospf-interface-ip">
    <type name="inet:ipv4-address"/>
  </typedef>
  <typedef name="ospf-route-dest">
    <type name="inet:ipv4-address"/>
  </typedef>
  <typedef name="ospf-route-prefix">
    <type name="inet:ipv4-address"/>
  </typedef>
  <typedef name="ospf-route-mask">
    <type name="inet:ipv4-address"/>
  </typedef>
  <grouping name="common-link-props">
    <container name="authentication-key">
      <container name="no-encrypt-auth-key-table">
        <leaf name="no-encrypt-auth-key">
          <type name="ospf-auth-psswd-string"/>
        </leaf>
      </container>
      <container name="auth-key-table">
        <leaf name="encrypttype">
          <type name="key-type"/>
        </leaf>
        <leaf name="auth-key">
          <type name="ospf-auth-psswd-string"/>
        </leaf>
      </container>
    </container>
    <leaf name="dead-interval">
      <default value="40"/>
      <type name="common-def:time-interval-sec">
        <range value="3..65535"/>
      </type>
    </leaf>
    <leaf name="hello-interval">
      <default value="10"/>
      <type name="common-def:time-interval-sec">
        <range value="1..65535"/>
      </type>
    </leaf>
    <leaf name="retransmit-interval">
      <type name="common-def:time-interval-sec">
        <range value="0..3600"/>
      </type>
    </leaf>
    <leaf name="transmit-delay">
      <type name="common-def:time-interval-sec">
        <range value="0..3600"/>
      </type>
    </leaf>
    <container name="md5-authentication">
      <leaf name="key-activation-wait-time">
        <type name="common-def:time-interval-sec">
          <range value="0..14400"/>
        </type>
      </leaf>
      <container name="no-encrypt-key-table">
        <leaf name="no-encrypt-key-id">
          <type name="uint32">
            <range value="1..255"/>
          </type>
        </leaf>
        <leaf name="no-encrypt-key">
          <type name="ospf-auth-psswd-string"/>
        </leaf>
      </container>
      <container name="key-table">
        <leaf name="key-id">
          <type name="uint32">
            <range value="1..255"/>
          </type>
        </leaf>
        <leaf name="key">
          <type name="key-type"/>
          <description>
            <text>MD5 Authentication password (key)</text>
          </description>
        </leaf>
        <leaf name="md5-authentication-key">
          <type name="ospf-auth-psswd-string"/>
        </leaf>
      </container>
    </container>
  </grouping>
  <grouping name="interface-fo-ospf">
