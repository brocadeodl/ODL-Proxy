  <typedef name="database-filter-options">
    <type name="enumeration">
      <enum name="allow-default-out">
        <value value="1"/>
      </enum>
      <enum name="allow-default-and-type4-out">
        <value value="2"/>
      </enum>
      <enum name="out">
        <value value="3"/>
      </enum>
    </type>
  </typedef>
