          <range value="0..14400"/>
        </type>
      </leaf>
      <uses name="common-link-props"/>
      <leaf name="cost">
        <default value="1"/>
        <type name="uint32">
          <range value="1..65535"/>
        </type>
      </leaf>
      <container name="database-filter">
        <leaf name="all-out">
          <type name="empty"/>
        </leaf>
        <leaf name="all-external">
          <type name="database-filter-options"/>
        </leaf>
        <leaf name="all-summary-external">
          <type name="database-filter-options"/>
        </leaf>
      </container>
      <leaf name="mtu-ignore">
        <type name="empty"/>
