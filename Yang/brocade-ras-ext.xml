  <augment target-node="/common-def:show">
    <container name="support"/>
    <container name="copy-support"/>
    <container name="infra">
      <description>
        <text>show system info</text>
      </description>
      <container name="process"/>
      <container name="environment"/>
    </container>
    <container name="logging"/>
  </augment>
  <container name="rasman"/>
  <rpc name="show-raslog">
    <description>
      <text>Shows the entries of RASLOG</text>
    </description>
    <input>
      <leaf name="rbridge-id">
        <type name="switchid-type"/>
      </leaf>
      <leaf name="number-of-latest-events">
        <type name="uint32"/>
        <description>
          <text>This specifies the number of recent events to be fetched
from RASLOG entries.</text>
        </description>
      </leaf>
    </input>
    <output>
      <list name="show-all-raslog">
        <leaf name="rbridge-id">
          <type name="switchid-type"/>
        </leaf>
        <leaf name="number-of-entries">
          <type name="uint32"/>
          <description>
            <text>This specifies the number of entries in the  be fetched</text>
          </description>
        </leaf>
        <list name="raslog-entries">
          <leaf name="index">
            <type name="uint32"/>
            <description>
              <text>Sequence number for the message</text>
            </description>
          </leaf>
          <leaf name="message-id">
            <type name="string"/>
            <description>
              <text>Message identifier</text>
            </description>
          </leaf>
          <leaf name="date-and-time-info">
            <type name="string"/>
            <description>
              <text>Date and time of the message.
The format is: YYYY-MM-DD/HH:MM:SS.SSSS (micro seconds)</text>
            </description>
          </leaf>
          <leaf name="severity">
            <type name="enumeration">
              <enum name="critical">
                <value value="1"/>
              </enum>
              <enum name="error">
                <value value="2"/>
              </enum>
              <enum name="warning">
                <value value="3"/>
              </enum>
              <enum name="informational">
                <value value="4"/>
              </enum>
              <enum name="debug">
                <value value="5"/>
              </enum>
              <enum name="unknown">
                <value value="6"/>
              </enum>
            </type>
            <reference>
              <text>swEventLevel defined in SW.mib</text>
            </reference>
            <description>
              <text>Severity of the message. Valid values include
INFO, WARNING, ERROR, and CRITICAL.</text>
            </description>
          </leaf>
          <leaf name="repeat-count">
            <type name="uint32"/>
            <description>
              <text>It identifies how many times this particular
event has occurred.</text>
            </description>
          </leaf>
          <leaf name="message">
            <type name="string"/>
            <description>
              <text>It contains the textual description of the event.</text>
            </description>
          </leaf>
          <leaf name="message-flag">
            <type name="enumeration">
              <enum name="other">
                <value value="1"/>
              </enum>
              <enum name="unknown">
                <value value="2"/>
              </enum>
              <enum name="ffdc">
                <value value="3"/>
              </enum>
              <enum name="vcs">
                <value value="4"/>
              </enum>
            </type>
            <description>
              <text>It specifies the type of the message.</text>
            </description>
          </leaf>
          <leaf name="log-type">
            <type name="enumeration">
              <enum name="system">
                <value value="0"/>
              </enum>
              <enum name="dce">
                <value value="1"/>
              </enum>
            </type>
            <description>
              <text>It specifies if the message is a SYSTEM or DCE log</text>
            </description>
          </leaf>
          <leaf name="switch-or-chassis-name">
            <type name="string"/>
            <description>
              <text>Switch name for the generator of this message, or chassis</text>
            </description>
          </leaf>
        </list>
      </list>
      <leaf name="cmd-status-error-msg">
        <type name="string"/>
      </leaf>
    </output>
  </rpc>
  <rpc name="show-support-save-status">
    <description>
      <text>Information on the status of recent support save request</text>
    </description>
    <input>
      <leaf name="rbridge-id">
        <type name="switchid-type"/>
      </leaf>
    </input>
    <output>
      <list name="show-support-save-status">
        <leaf name="rbridge-id">
          <type name="switchid-type"/>
        </leaf>
        <leaf name="status">
          <type name="enumeration">
            <enum name="other">
              <value value="1"/>
            </enum>
            <enum name="unknown">
              <value value="2"/>
            </enum>
            <enum name="completed">
              <value value="3"/>
            </enum>
            <enum name="inProgress">
              <value value="4"/>
            </enum>
            <enum name="error">
              <value value="5"/>
            </enum>
          </type>
          <description>
            <text>status of recent support save.</text>
          </description>
        </leaf>
        <leaf name="message">
          <type name="string"/>
          <description>
            <text>Textual description of status of recent support save is
as follows:
 unknown/other - Support Save is failed for
                 Unknown/undefined reason.  
 completed - The recent support save is completed successfully.
 in-progress - The support save is not yet completed.
               It may contain the value of % completed.
 error - Specify the error message.</text>
          </description>
        </leaf>
        <leaf name="percentage-of-completion">
          <type name="uint32"/>
          <description>
            <text>It specifies the value of % of completion.</text>
          </description>
        </leaf>
      </list>
    </output>
  </rpc>
  <rpc name="show-system-info">
    <description>
      <text>Shows the system information MAC etc.</text>
    </description>
    <input>
      <leaf name="rbridge-id">
        <type name="switchid-type"/>
      </leaf>
    </input>
    <output>
      <list name="show-system-info">
        <leaf name="rbridge-id">
          <type name="switchid-type"/>
        </leaf>
        <leaf name="stack-mac">
          <type name="ietfyang:mac-address"/>
          <description>
            <text>MAC address of the switch.</text>
          </description>
        </leaf>
      </list>
    </output>
  </rpc>
  <augment target-node="/common-def:clear">
    <container name="support"/>
    <container name="logging"/>
  </augment>
</module>
