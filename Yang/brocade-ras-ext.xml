      <enum name="other">
        <value value="1"/>
      </enum>
      <enum name="unknown">
        <value value="2"/>
      </enum>
      <enum name="on">
        <value value="3"/>
      </enum>
      <enum name="off">
        <value value="4"/>
      </enum>
      <enum name="faulty">
        <value value="5"/>
      </enum>
      <enum name="inserted">
        <value value="6"/>
      </enum>
      <enum name="ready">
        <value value="7"/>
      </enum>
      <enum name="up">
        <value value="8"/>
      </enum>
