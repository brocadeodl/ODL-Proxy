  <typedef name="switchid-type">
    <type name="string">
      <length value="1..3">
        <error-message>
          <value>Invalid rbridge-id (must be in the range 1 - 239) or all</value>
        </error-message>
      </length>
    </type>
  </typedef>
  <typedef name="switchid-type-noall">
    <type name="string">
      <length value="1..3">
        <error-message>
          <value>Invalid rbridge-id (must be in the range 1 - 239)</value>
        </error-message>
      </length>
    </type>
  </typedef>
  <typedef name="rbridge-ids-all-type">
    <description>
      <text>The unique identifier for a switch.  The usage of `all'
refers to `all' rbridge-id's in the cluster.</text>
    </description>
    <type name="union">
      <type name="common-def:ui32-range"/>
      <type name="string">
        <pattern value="all"/>
      </type>
    </type>
  </typedef>
  <typedef name="ha-recovery-type">
    <type name="enumeration">
      <enum name="unknown">
        <value value="0"/>
      </enum>
      <enum name="warm-recovery-in-progress">
        <value value="1"/>
      </enum>
      <enum name="cold-recovery-in-progress">
        <value value="2"/>
      </enum>
      <enum name="warm-recovered">
        <value value="3"/>
      </enum>
      <enum name="cold-recovered">
        <value value="4"/>
      </enum>
    </type>
  </typedef>
  <typedef name="fru-type">
    <type name="enumeration">
      <enum name="other">
        <value value="1"/>
      </enum>
      <enum name="unknown">
        <value value="2"/>
      </enum>
      <enum name="chassis">
        <value value="3"/>
      </enum>
      <enum name="cp">
        <value value="4"/>
      </enum>
      <enum name="other-cp">
        <value value="5"/>
      </enum>
      <enum name="switch-blade">
        <value value="6"/>
      </enum>
      <enum name="wwn">
        <value value="7"/>
      </enum>
      <enum name="power-suppy">
        <value value="8"/>
      </enum>
      <enum name="fan">
        <value value="9"/>
      </enum>
      <enum name="core-blade">
        <value value="10"/>
      </enum>
      <enum name="application-blade">
        <value value="11"/>
      </enum>
    </type>
  </typedef>
  <typedef name="fru-operational-status">
