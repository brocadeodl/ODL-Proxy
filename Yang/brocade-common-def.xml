  <typedef name="ui32-range">
    <type name="string">
      <pattern value="[0-9]+(-[0-9]+)?(,[0-9]+(-[0-9]+)?)*"/>
    </type>
  </typedef>
  <typedef name="slot-id-type">
    <type name="string">
      <length value="1..16">
        <description>
          <text>WORD:1-16;;Slot name in [mapped ID]/slot format.</text>
        </description>
      </length>
    </type>
  </typedef>
  <typedef name="chip-id-type">
    <type name="uint8">
      <range value="1..9">
        <description>
          <text>Chip-ID;;  1..9</text>
        </description>
      </range>
    </type>
  </typedef>
  <typedef name="slot-chip-id-type">
    <type name="string">
      <length value="3..16"/>
    </type>
  </typedef>
  <typedef name="rbridge-id-type">
    <type name="uint32">
      <range value="1..239">
        <error-message>
          <value>Invalid rbridgeId (must be in the range 1-239).</value>
        </error-message>
      </range>
    </type>
  </typedef>
  <typedef name="rbridge-id-all-type">
    <type name="union">
      <type name="rbridge-id-type"/>
      <type name="string">
        <pattern value="all"/>
      </type>
    </type>
  </typedef>
  <typedef name="rbridge-id-all-range-type">
    <type name="union">
      <type name="rbridge-id-type"/>
      <type name="string"/>
    </type>
  </typedef>
  <typedef name="domain-id-type">
    <type name="uint32">
      <range value="1..239">
        <error-message>
          <value>Invalid domainId (must be in the range 1..239)</value>
        </error-message>
      </range>
    </type>
  </typedef>
  <typedef name="mcast-priority-type">
    <type name="uint32">
      <range value="1..255">
        <error-message>
          <value>Invalid Priority Value (must be in the range 1..255)</value>
        </error-message>
      </range>
    </type>
  </typedef>
  <typedef name="idid-mode-type">
    <type name="enumeration">
      <enum name="disable"/>
      <enum name="enable"/>
    </type>
  </typedef>
  <typedef name="mac-address-mcast-type">
    <type name="string">
      <pattern value="[0-9a-fA-F][13579BDFbdf][0-9a-fA-F][0-9a-fA-F]\.[0-9a-fA-F][0-9a-fA-F][0-9a-fA-F][0-9a-fA-F]\.[0-9a-fA-F][0-9a-fA-F][0-9a-fA-F][0-9a-fA-F]"/>
