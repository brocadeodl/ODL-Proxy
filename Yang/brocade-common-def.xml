  <typedef name="time-interval-day">
    <type name="uint32"/>
    <units name="Days"/>
  </typedef>
  <typedef name="time-interval-hour">
    <type name="uint32"/>
    <units name="Hours"/>
  </typedef>
  <typedef name="time-interval-minute">
    <type name="uint32"/>
    <units name="Minutes"/>
  </typedef>
  <typedef name="time-interval-sec">
    <type name="uint32"/>
    <units name="Seconds"/>
  </typedef>
  <typedef name="time-interval-msec">
    <type name="uint32"/>
    <units name="milliSeconds"/>
  </typedef>
