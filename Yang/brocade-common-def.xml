  <typedef name="lc-number-type">
    <type name="uint16">
      <range value="0..65535"/>
    </type>
    <description>
      <text>The lc-number-type represents a 16-bit line card number in the managed entity.</text>
    </description>
  </typedef>
  <typedef name="inet-address-type">
    <type name="enumeration">
      <enum name="unknown">
        <value value="0"/>
      </enum>
      <enum name="ipv4">
        <value value="1"/>
      </enum>
      <enum name="ipv6">
        <value value="2"/>
      </enum>
      <enum name="ipv4z">
        <value value="3"/>
      </enum>
      <enum name="ipv6z">
        <value value="4"/>
      </enum>
      <enum name="dns">
        <value value="16"/>
      </enum>
    </type>
  </typedef>
  <grouping name="inet-address">
    <description>
      <text>A generic address group. The value of an 'address'
must always be consistent with the value of the 
associated 'type' leaf.</text>
    </description>
    <leaf name="address-type">
      <type name="inet-address-type"/>
    </leaf>
    <leaf name="address">
      <type name="inet:host"/>
    </leaf>
  </grouping>
  <typedef name="ipv4-mask">
    <type name="string">
      <pattern value="(([0]|25[0-5])\.){3}([0]|25[0-5])"/>
    </type>
  </typedef>
  <typedef name="ipv4-prefix-mask">
    <type name="string">
      <pattern value="(([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\.)(([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\.){2}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])/(([1-9])|([1-2][0-9])|(3[0-1]))">
        <error-message>
          <value>Invalid IPv4 Address/Mask format</value>
        </error-message>
      </pattern>
    </type>
  </typedef>
  <typedef name="vrf-name">
    <type name="string">
      <pattern value="((([a-zA-Z0-9_]([a-zA-Z0-9\-_]){0,61})?[a-zA-Z0-9]\.)*([a-zA-Z0-9_]([a-zA-Z0-9\-_]){0,61})?[a-zA-Z0-9]\.?)|\."/>
      <length value="1..253"/>
    </type>
    <reference>
      <text>L. Andersson, 'Provider Provisioned Virtual Private 
Network (VPN) Terminology', RFC 4026, March 2005.</text>
    </reference>
  </typedef>
