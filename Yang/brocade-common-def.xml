  <typedef name="name-string32">
    <type name="string">
      <pattern value="[a-zA-Z]{1}([-a-zA-Z0-9\.\\\\@#\+\*\(\)=\{~\}%&lt;&gt;=$_\[\]\|]{0,31})"/>
    </type>
  </typedef>
  <typedef name="name-string64">
    <type name="string">
      <pattern value="[a-zA-Z]{1}([-a-zA-Z0-9\.\\\\@#\+\*\(\)=\{~\}%&lt;&gt;=$_\[\]\|]{0,63})"/>
    </type>
  </typedef>
  <typedef name="name-string63">
    <type name="string">
      <pattern value="[a-zA-Z]{1}([-a-zA-Z0-9\.\\\\@#\+\*\(\)=\{~\}%&lt;&gt;=$_\[\]\|]{0,62})"/>
    </type>
  </typedef>
  <typedef name="name-string128">
    <type name="string">
      <pattern value="[a-zA-Z]{1}([-a-zA-Z0-9\.\\\\@#\+\*\(\)=\{~\}%&lt;&gt;=$_\[\]\|]{0,127})"/>
    </type>
  </typedef>
  <typedef name="wwn-type">
    <type name="string">
      <pattern value="[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]"/>
    </type>
    <reference>
      <text>t11 standard, Fibre Channel Framing and Signaling (FC-FS) 
Rev 1.70 - Section 14 Name_Indentifier Formats.</text>
    </reference>
  </typedef>
