<?xml version="1.0" encoding="UTF-8"?>
<module name="brocade-pim"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:pim="urn:brocade.com:mgmt:brocade-pim"
        xmlns:rbridge="urn:brocade.com:mgmt:brocade-rbridge"
        xmlns:interface="urn:brocade.com:mgmt:brocade-interface"
        xmlns:ip-config="urn:brocade.com:mgmt:brocade-ip-config"
        xmlns:common-def="urn:brocade.com:mgmt:brocade-common-def"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types">
  <namespace uri="urn:brocade.com:mgmt:brocade-pim"/>
  <prefix value="pim"/>
  <import module="brocade-rbridge">
    <prefix value="rbridge"/>
  </import>
  <import module="brocade-interface">
    <prefix value="interface"/>
  </import>
  <import module="brocade-ip-config">
    <prefix value="ip-config"/>
  </import>
  <import module="brocade-common-def">
    <prefix value="common-def"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <organization>
    <text>Brocade Communications Systems, Inc.</text>
  </organization>
  <contact>
    <text>Brocade Communications Systems, Inc.
130 Holger Way,
San Jose, CA 95134
U.S.A
Tel: +1-408-333-8000
E-mail: info@brocade.com
Web: www.brocade.com</text>
  </contact>
  <description>
    <text>This file describes the data modeliing of all
the configuration commands for the multicast 
protocol, Protocol Independent Multicast (PIM).</text>
  </description>
  <revision date="2011-06-01">
    <description>
      <text>Initial version of PIM module</text>
    </description>
  </revision>
  <typedef name="spt-thr-type">
    <type name="union">
      <type name="uint32">
        <range value="1..4294967295"/>
      </type>
      <type name="enumeration">
        <enum name="infinity">
          <value value="0"/>
        </enum>
      </type>
    </type>
  </typedef>
  <typedef name="pim-src-grp-ipv4-addr">
    <type name="inet:ipv4-address"/>
  </typedef>
  <typedef name="pim-grp-prefix-addr">
    <type name="inet:ipv4-address"/>
  </typedef>
  <typedef name="pim-ipv4-addr">
    <type name="string">
      <pattern value="((0|(1[0-9]{0,2})|(2(([0-4][0-9]?)|(5[0-5]?)|([6-9]?)))|([3-9][0-9]?))\.){3}(0|(1[0-9]{0,2})|(2(([0-4][0-9]?)|(5[0-5]?)|([6-9]?)))|([3-9][0-9]?))"/>
    </type>
  </typedef>
  <typedef name="pim-intf-types">
    <type name="enumeration">
      <enum name="gigabitethernet"/>
      <enum name="fortygigabitethernet"/>
      <enum name="tengigabitethernet"/>
      <enum name="port-channel"/>
      <enum name="ve"/>
    </type>
  </typedef>
  <typedef name="grp-prefix-action">
    <type name="enumeration">
      <enum name="add">
        <value value="1"/>
      </enum>
      <enum name="delete">
        <value value="2"/>
      </enum>
    </type>
  </typedef>
  <grouping name="rtr-pim-cfg-cmd">
    <leaf name="max-mcache">
      <type name="uint32">
        <range value="1..2048"/>
      </type>
      <default value="2048"/>
    </leaf>
    <leaf name="hello-interval">
      <type name="int16">
        <range value="10..3600"/>
      </type>
      <units name="Seconds"/>
      <default value="30"/>
    </leaf>
    <leaf name="nbr-timeout">
      <type name="int16">
        <range value="35..12600"/>
      </type>
      <default value="105"/>
    </leaf>
    <leaf name="inactivity-timer">
      <type name="int16">
        <range value="60..3600"/>
      </type>
      <default value="180"/>
    </leaf>
    <leaf name="message-interval">
      <type name="uint32">
        <range value="10..65535"/>
      </type>
      <units name="Seconds"/>
      <default value="60"/>
    </leaf>
    <leaf name="spt-threshold">
      <type name="spt-thr-type"/>
      <default value="1"/>
    </leaf>
    <container name="bsr-candidate">
      <list name="bsr-cand-interface">
        <key value="bsr-cand-intf-type bsr-cand-intf-id"/>
        <leaf name="bsr-cand-intf-type">
          <type name="pim-intf-types"/>
        </leaf>
        <leaf name="bsr-cand-intf-id">
          <type name="string"/>
        </leaf>
        <leaf name="hash-mask-length">
          <type name="uint16">
            <range value="1..32"/>
          </type>
        </leaf>
        <leaf name="bsr-cand-priority">
          <type name="uint32">
            <range value="0..255"/>
          </type>
          <mandatory value="false"/>
        </leaf>
      </list>
    </container>
    <container name="rp-candidate">
      <list name="rp-cand-interface">
        <key value="rp-cand-intf-type rp-cand-intf-id"/>
        <leaf name="rp-cand-intf-type">
          <type name="pim-intf-types"/>
        </leaf>
        <leaf name="rp-cand-intf-id">
          <type name="string"/>
        </leaf>
      </list>
      <list name="rp-cand-grp-prefix">
        <key value="rp-cand-grp-prefix-action rp-cand-grp-prefix-ip rp-cand-grp-prefix-length"/>
        <leaf name="rp-cand-grp-prefix-action">
          <type name="grp-prefix-action"/>
        </leaf>
        <leaf name="rp-cand-grp-prefix-ip">
          <type name="pim-grp-prefix-addr"/>
        </leaf>
        <leaf name="rp-cand-grp-prefix-length">
          <type name="uint16">
            <range value="1..32"/>
          </type>
        </leaf>
      </list>
    </container>
    <list name="rp-address">
      <key value="rp-ip-addr"/>
      <leaf name="rp-ip-addr">
        <type name="inet:ipv4-address"/>
      </leaf>
      <leaf name="prefix-list">
        <type name="string">
          <length value="1..64"/>
        </type>
      </leaf>
    </list>
  </grouping>
  <grouping name="pim-intf-cfg-cmd">
    <container name="pim-int-cmd">
      <leaf name="pim-sparse">
        <type name="empty"/>
      </leaf>
      <container name="pim">
        <leaf name="dr-priority">
          <type name="uint32">
            <range value="0..65535"/>
          </type>
          <default value="1"/>
        </leaf>
        <leaf name="ttl-threshold">
          <type name="uint32">
            <range value="1..64"/>
          </type>
          <default value="1"/>
        </leaf>
      </container>
      <leaf name="multicast-boundary">
        <type name="empty"/>
      </leaf>
    </container>
  </grouping>
  <grouping name="pim-intf-phy-cfg">
    <container name="pim-intf-phy-cont">
      <uses name="pim-intf-cfg-cmd"/>
    </container>
  </grouping>
  <grouping name="pim-intf-vlan-cfg">
    <container name="pim-intf-vlan-cont">
      <uses name="pim-intf-cfg-cmd"/>
    </container>
  </grouping>
  <grouping name="pim-intf-po-cfg">
    <container name="pim-intf-po-cont">
      <uses name="pim-intf-cfg-cmd"/>
    </container>
  </grouping>
  <grouping name="global-pim-cmds">
    <container name="pim">
      <presence value="true"/>
      <uses name="rtr-pim-cfg-cmd"/>
    </container>
  </grouping>
  <augment target-node="/rbridge:rbridge-id/rbridge:router">
    <container name="hide-pim-holder">
      <uses name="global-pim-cmds"/>
    </container>
  </augment>
  <augment target-node="/interface:interface/interface:tengigabitethernet/interface:ip">
    <uses name="pim-intf-phy-cfg"/>
  </augment>
  <augment target-node="/interface:interface/interface:fortygigabitethernet/interface:ip">
    <uses name="pim-intf-phy-cfg"/>
  </augment>
  <augment target-node="/interface:interface/interface:gigabitethernet/interface:ip">
    <uses name="pim-intf-phy-cfg"/>
  </augment>
  <augment target-node="/rbridge:rbridge-id/interface:interface/interface:ve/ip-config:ip">
    <uses name="pim-intf-vlan-cfg"/>
  </augment>
  <augment target-node="/interface:interface-vlan/interface:interface/interface:ve/interface:ip">
    <uses name="pim-intf-vlan-cfg"/>
  </augment>
</module>
