<?xml version="1.0" encoding="UTF-8"?>
<module name="brocade-igmp"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:igmp="urn:brocade.com:mgmt:brocade-igmp"
        xmlns:rbridge="urn:brocade.com:mgmt:brocade-rbridge"
        xmlns:interface="urn:brocade.com:mgmt:brocade-interface"
        xmlns:ip-config="urn:brocade.com:mgmt:brocade-ip-config"
        xmlns:igmp-snooping="urn:brocade.com:mgmt:brocade-igmp-snooping"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types">
  <namespace uri="urn:brocade.com:mgmt:brocade-igmp"/>
  <prefix value="igmp"/>
  <import module="brocade-rbridge">
    <prefix value="rbridge"/>
  </import>
  <import module="brocade-interface">
    <prefix value="interface"/>
  </import>
  <import module="brocade-ip-config">
    <prefix value="ip-config"/>
  </import>
  <import module="brocade-igmp-snooping">
    <prefix value="igmp-snooping"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <organization>
    <text>Brocade Communications Systems, Inc.</text>
  </organization>
  <contact>
    <text>Brocade Communications Systems, Inc.
130 Holger Way,
San Jose, CA 95134
U.S.A
Tel: +1-408-333-8000
E-mail: info@brocade.com
Web: www.brocade.com</text>
  </contact>
  <description>
    <text>This file defines the data model for the IGMP (Internet
Group Management Protocol) configuration commands.</text>
  </description>
  <revision date="2011-03-28">
    <description>
      <text>Initial version of the IGMP module.</text>
    </description>
  </revision>
  <grouping name="igmp-phy-intf-cfg-cmd">
    <container name="igmp-phy-intf-cfg">
      <container name="igmp">
        <leaf name="last-member-query-interval">
          <type name="igmp-snooping:lmqt-type"/>
          <units name="milliSeconds"/>
          <default value="1000"/>
        </leaf>
        <leaf name="query-interval">
          <type name="igmp-snooping:qi-type"/>
          <units name="Seconds"/>
          <default value="125"/>
        </leaf>
        <leaf name="query-max-response-time">
          <type name="igmp-snooping:qmrt-type"/>
          <units name="Seconds"/>
          <default value="10"/>
        </leaf>
        <leaf name="immediate-leave">
          <type name="empty"/>
        </leaf>
        <list name="static-group">
          <key value="sg-addr"/>
          <leaf name="sg-addr">
            <type name="inet:ipv4-address"/>
          </leaf>
        </list>
      </container>
    </container>
  </grouping>
  <grouping name="igmp-po-intf-cfg-cmd">
    <container name="igmp-po-intf-cfg">
      <container name="igmp">
        <leaf name="last-member-query-interval">
          <type name="igmp-snooping:lmqt-type"/>
          <units name="milliSeconds"/>
          <default value="1000"/>
        </leaf>
        <leaf name="query-interval">
          <type name="igmp-snooping:qi-type"/>
          <units name="Seconds"/>
          <default value="125"/>
        </leaf>
        <leaf name="query-max-response-time">
          <type name="igmp-snooping:qmrt-type"/>
          <units name="Seconds"/>
          <default value="10"/>
        </leaf>
        <leaf name="immediate-leave">
          <type name="empty"/>
        </leaf>
        <list name="static-group">
          <key value="sg-addr"/>
          <leaf name="sg-addr">
            <type name="inet:ipv4-address"/>
          </leaf>
        </list>
      </container>
    </container>
  </grouping>
  <grouping name="vlan_igmp_cmds">
    <container name="igmp">
      <leaf name="last-member-query-interval">
        <type name="igmp-snooping:lmqt-type"/>
        <units name="milliSeconds"/>
        <default value="1000"/>
      </leaf>
      <leaf name="query-interval">
        <type name="igmp-snooping:qi-type"/>
        <units name="Seconds"/>
        <default value="125"/>
      </leaf>
      <leaf name="query-max-response-time">
        <type name="igmp-snooping:qmrt-type"/>
        <units name="Seconds"/>
        <default value="10"/>
      </leaf>
      <leaf name="immediate-leave">
        <type name="empty"/>
      </leaf>
      <list name="static-group">
        <key value="sg-addr"/>
        <leaf name="sg-addr">
          <type name="inet:ipv4-address"/>
        </leaf>
      </list>
    </container>
  </grouping>
  <augment target-node="/rbridge:rbridge-id/interface:interface/interface:ve/ip-config:ip">
    <uses name="vlan_igmp_cmds"/>
  </augment>
  <augment target-node="/interface:interface-vlan/interface:interface/interface:ve/interface:ip">
    <uses name="vlan_igmp_cmds"/>
  </augment>
  <augment target-node="/interface:interface/interface:gigabitethernet/interface:ip">
    <description>
      <text>Internet Protocol (IP)</text>
    </description>
    <uses name="igmp-phy-intf-cfg-cmd"/>
  </augment>
  <augment target-node="/interface:interface/interface:tengigabitethernet/interface:ip">
    <description>
      <text>Internet Protocol (IP)</text>
    </description>
    <uses name="igmp-phy-intf-cfg-cmd"/>
  </augment>
  <augment target-node="/interface:interface/interface:fortygigabitethernet/interface:ip">
    <description>
      <text>Internet Protocol (IP)</text>
    </description>
    <uses name="igmp-phy-intf-cfg-cmd"/>
  </augment>
</module>
