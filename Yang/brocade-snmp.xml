<?xml version="1.0" encoding="UTF-8"?>
<module name="brocade-snmp"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:snmp-server="urn:brocade.com:mgmt:brocade-snmp"
        xmlns:rbridge="urn:brocade.com:mgmt:brocade-rbridge"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types"
        xmlns:common-def="urn:brocade.com:mgmt:brocade-common-def">
  <namespace uri="urn:brocade.com:mgmt:brocade-snmp"/>
  <prefix value="snmp-server"/>
  <import module="brocade-rbridge">
    <prefix value="rbridge"/>
  </import>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="brocade-common-def">
    <prefix value="common-def"/>
  </import>
  <organization>
    <text>Brocade Communications Systems, Inc.</text>
  </organization>
  <contact>
    <text>Address :  120 Holger Way
           San Jose, CA 95134
           U.S.A
Phone    :  +1-408-333-8000
E-mail   :  support@brocade.com
Website  :  www.brocade.com</text>
  </contact>
  <description>
    <text>Data Model for SNMP Server and related configuration CLIs</text>
  </description>
  <revision date="2011-03-18">
    <description>
      <text>Formatting yang model according to guidelines</text>
    </description>
  </revision>
  <typedef name="host-port">
    <type name="uint32">
      <range value="0 .. 65535">
        <error-message>
          <value>Port number should be in the range 0-65535</value>
        </error-message>
      </range>
    </type>
    <default value="162"/>
  </typedef>
  <typedef name="host-version">
    <type name="enumeration">
      <enum name="1">
        <value value="0"/>
      </enum>
      <enum name="2c">
        <value value="1"/>
      </enum>
    </type>
  </typedef>
  <typedef name="community-access">
    <type name="enumeration">
      <enum name="ro">
        <value value="0"/>
      </enum>
      <enum name="rw">
        <value value="1"/>
      </enum>
    </type>
  </typedef>
  <typedef name="snmp-engineID">
    <type name="string">
      <pattern value="[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]:[0-9a-fA-F][0-9a-fA-F]"/>
    </type>
  </typedef>
  <typedef name="system-contact">
    <type name="string">
      <length value="4 .. 256">
        <error-message>
          <value>Invalid Contact String length. String size 
must be between 4 and 255</value>
        </error-message>
      </length>
    </type>
  </typedef>
  <typedef name="system-location">
    <type name="string">
      <length value="4 .. 256">
        <error-message>
          <value>Invalid Location String length. String size
must be between 4 and 255</value>
        </error-message>
      </length>
    </type>
  </typedef>
  <typedef name="system-description">
    <type name="string">
      <length value="4 .. 256">
        <error-message>
          <value>Invalid System Description String length.
String size must be between 4 and 255</value>
        </error-message>
      </length>
    </type>
  </typedef>
  <typedef name="sev-level">
    <type name="enumeration">
      <enum name="None"/>
      <enum name="Critical"/>
      <enum name="Error"/>
      <enum name="Warning"/>
      <enum name="Info"/>
      <enum name="Debug"/>
    </type>
    <default value="None"/>
  </typedef>
  <container name="snmp-server">
    <list name="context">
      <description>
        <text>provides the  mapping of SNMP context (represented by
the value of vacmContextName) to the various entities
within the entities within the managed device</text>
      </description>
      <key value="context-name"/>
      <max-elements value="256"/>
      <leaf name="context-name">
        <type name="string"/>
      </leaf>
      <leaf name="vrf-name">
        <type name="string"/>
      </leaf>
    </list>
    <list name="community">
      <key value="community"/>
      <max-elements value="256"/>
      <leaf name="community">
        <type name="string"/>
      </leaf>
      <leaf name="access">
        <type name="community-access"/>
        <default value="ro"/>
      </leaf>
    </list>
    <list name="user">
      <key value="username"/>
      <max-elements value="6"/>
      <leaf name="username">
        <type name="string">
          <length value="1 .. 16">
            <error-message>
              <value>Username length should be in the range 1-16</value>
            </error-message>
          </length>
        </type>
      </leaf>
      <leaf name="groupname">
        <type name="string"/>
        <default value="snmpuser"/>
      </leaf>
      <leaf name="auth">
        <type name="enumeration">
          <enum name="md5">
            <value value="0"/>
          </enum>
          <enum name="sha">
            <value value="1"/>
          </enum>
          <enum name="noauth">
            <value value="2"/>
          </enum>
        </type>
        <default value="noauth"/>
      </leaf>
      <leaf name="auth-password">
        <type name="string"/>
        <default value=""/>
      </leaf>
      <leaf name="priv">
        <type name="enumeration">
          <enum name="DES">
            <value value="0"/>
          </enum>
          <enum name="nopriv">
            <value value="1"/>
          </enum>
          <enum name="AES128">
            <value value="2"/>
          </enum>
        </type>
        <default value="nopriv"/>
      </leaf>
      <leaf name="priv-password">
        <type name="string"/>
        <default value=""/>
      </leaf>
      <leaf name="encrypted">
        <type name="empty"/>
        <mandatory value="false"/>
      </leaf>
    </list>
    <list name="v3host">
      <key value="hostip username"/>
      <max-elements value="6"/>
      <leaf name="hostip">
        <type name="inet:host"/>
      </leaf>
      <leaf name="username">
        <type name="string"/>
      </leaf>
      <leaf name="udp-port">
        <type name="host-port"/>
      </leaf>
      <leaf name="notifytype">
        <type name="enumeration">
          <enum name="traps">
            <value value="0"/>
          </enum>
          <enum name="informs">
            <value value="1"/>
          </enum>
        </type>
        <default value="traps"/>
      </leaf>
      <leaf name="engineid">
        <type name="string"/>
        <default value="00:00:00:00:00:00:00:00:00"/>
      </leaf>
      <leaf name="severity-level">
        <type name="sev-level"/>
      </leaf>
    </list>
    <list name="host">
      <key value="ip community"/>
      <max-elements value="12"/>
      <leaf name="ip">
        <type name="inet:host"/>
      </leaf>
      <leaf name="version">
        <type name="host-version"/>
        <default value="1"/>
      </leaf>
      <leaf name="community">
        <type name="string"/>
      </leaf>
      <leaf name="udp-port">
        <type name="host-port"/>
      </leaf>
      <leaf name="severity-level">
        <type name="sev-level"/>
      </leaf>
    </list>
    <container name="agtconfig">
      <leaf name="contact">
        <type name="system-contact"/>
      </leaf>
      <leaf name="location">
        <type name="system-location"/>
      </leaf>
      <leaf name="sys-descr">
        <type name="system-description"/>
      </leaf>
    </container>
    <container name="engineID-drop">
      <uses name="engineID-group"/>
    </container>
  </container>
  <grouping name="engineID-group">
    <container name="engineID">
      <leaf name="local">
        <type name="snmp-engineID"/>
      </leaf>
    </container>
  </grouping>
  <augment target-node="/rbridge:rbridge-id">
    <container name="snmp-server">
      <uses name="engineID-group"/>
    </container>
  </augment>
</module>
