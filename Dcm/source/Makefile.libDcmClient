DCM_CLIENT_LIB.SO.MAJOR := $(DCM_CLIENT_LIB.SO).$(DCM_VERSION_MAJOR)
DCM_CLIENT_LIB.SO.VER := \
	$(DCM_CLIENT_LIB.SO.MAJOR).$(DCM_VERSION_MINOR).$(DCM_VERSION_PATCH)


.SECONDEXPANSION:
################################################################################
#the links should not depend on their targets, and in fact may be created to not-yet existing targets
# the order-only dependency is necessary only because gnu make accesses
# a link target to cache it, and emake behaves compatibly
################################################################################

$(DCM_CLIENT_LIB.SO) $(DCM_CLIENT_LIB.SO.MAJOR):  | $$(@D)/. $(DCM_CLIENT_LIB.SO.VER)
	ln -sf $(notdir $(DCM_CLIENT_LIB.SO.VER)) $@


$(DCM_CLIENT_LIB.SO.VER): $(SINGLE_OBJ_FILES) $(BUILD_DIR_OBJ)/DcmVersion.o $(BUILD_DIR_OBJ)/DcmResourceIds.o \
	$(MKFS) | $$(@D)/.
	@echo -e "Linking DcmClient objects "
	@echo -e "`echo $(SINGLE_OBJ_FILES)|sed -e 's/ /\n/g'`  "
	$(CXX) -shared -fPIC -Wl,-soname,lib$(DCM_CLIENT_PROJECT_NAME).so $(ZVM_LDFLAGS) -o $@ \
		$(filter %.o, $^)
	@echo -e "DcmClient library generated\n"
