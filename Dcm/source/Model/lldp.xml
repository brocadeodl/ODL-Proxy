            </Member>
            <Member name="multiplier" dataType="UI32">
            </Member>        
            <Member name="dcbxFcoeAppTlv" dataType="bool">
            </Member>
            <Member name="dcbxFcoeLogicalLinkTlv" dataType="bool">
            </Member>   
            <Member name="dcbxIscsiAppTlv" dataType="bool">
            </Member>
            <Member name="dot1xTlv" dataType="bool">
            </Member>  
            <Member name="dot3xTlv" dataType="bool">
            </Member>  
            
            <!-- The following are Optional TLVs -->
            <Member name="mgmtAddrTlv" dataType="bool">
