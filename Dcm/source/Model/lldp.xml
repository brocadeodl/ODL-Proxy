            <Member name="mgmtFlags" dataType="UI32">
            </Member>
            <Member name="cmdcode" dataType="UI32">
            </Member>
            <Member name="msgType" dataType="UI32">
            </Member>  
            
            <Member name="ProfileName" dataType="string">
            </Member>
            <Member name="description" dataType="string"> 
